# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/google_voice_dialer
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=your_password
PGDATABASE=google_voice_dialer

# Session Secret (generate a random string)
SESSION_SECRET=your-random-secret-key-here

# Google Voice Credentials
GOOGLE_VOICE_EMAIL=your-google-voice-email@gmail.com
GOOGLE_VOICE_PASSWORD=your-google-voice-password

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Virtual Audio Cable Device Names (Windows VPS)
# These are the default device names for VB-Audio Virtual Cable
# Adjust these if your Virtual Audio Cable has different names
VAC_INPUT_DEVICE=CABLE Output (VB-Audio Virtual Cable)
VAC_OUTPUT_DEVICE=CABLE Input (VB-Audio Virtual Cable)
